<?xml version="1.0"?>
<!--
/**
 * Layout override for Customer account dashboard.
 * This is an example of how to override store layout and show Gigya edit profile form.
 * Use this block name to insert the block wherever you want.
 */
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Gigya\GigyaIM\Block\Account\Dashboard\GigyaInfo" name="gigya_customer_account_dashboard_info" as="gigya_info" template="Gigya_GigyaIM::account/dashboard/gigya_info.phtml" cacheable="false"  ifconfig="gigya_section/general/enable_gigya" />
        </referenceContainer>
    </body>
</page>
